import math

def find_max_min(arr):
    if len(arr) == 0:
        return None, None  
    
    max_num = max(arr)
    min_num = min(arr)
    
    return max_num, min_num

arr1 = [1, 2, 3, 4, 5]
max1, min1 = find_max_min(arr1)
print(f"Input: {arr1}")
print(f"Maximum is: {max1}")
print(f"Minimum is: {min1}")

arr2 = [5, 3, 7, 4, 2]
max2, min2 = find_max_min(arr2)
print(f"Input: {arr2}")
print(f"Maximum is: {max2}")
print(f"Minimum is: {min2}")
